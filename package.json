{
  "name": "jenny-database",
  "version": "0.1.0",
  "description": "Generic database objects for use with jenny",
  "author": {
    "name": "Ryan Grady",
    "email": "ryan@angry-yard.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angry-yard/database.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angry-yard/database/issues"
  },
  "homepage": "https://github.com/angry-yard/database",
  "scripts": {
    "build": "npm run copyJs && npm run copyFiles && npm run cleanDist",
    "copyJs": "xcopy src\\*.js dist /s /y",
    "copyFiles": "copy README.md dist && copy package.json dist",
    "cleanDist": "rd dist\\interfaces /s /q && rd dist\\strings\\interfaces /s /q",
    "publish": "npm publish .\\dist"
  },
  "dependencies": {
    "bluebird": "^3.4.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/node": "^6.0.41",
    "typescript": "^2.0.3"
  }
}